{
  "name": "pmhc.co",
  "version": "1.0.0",
  "description": "2015 Portfolio",
  "main": "server.js",
  "scripts": {
    "start-dev": "cross-env NODE_ENV=development webpack --watch --config webpack.server.config.js",
    "start-client": "cross-env NODE_ENV=development webpack-dev-server --port 8002 --hot --inline --config webpack.dev.config.js",
    "start": "npm run start-dev & npm run start-client & node ./src/server.dev.js",
    "start-win": "start \"ServerWatcher\" npm run start-dev & start \"ClientWatcher\" npm run start-client & start \"Server\" node ./src/server.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js -p",
    "deploy": "cross-env NODE_ENV=production node ./build/server.js"
  },
  "author": "Philip Chung",
  "license": "ISC",
